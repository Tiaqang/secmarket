<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/container_bg"
    tools:context="com.project.my.secmarket.Fragment3">

    <!-- TODO: Update blank fragment layout -->
   <RelativeLayout
       android:id="@+id/my_layout_info"
       android:layout_marginTop="@dimen/dividing_10"
       android:layout_width="match_parent"
       android:layout_height="100dp"
       android:background="@color/white">
       <ImageView
           android:id="@+id/my_head"
           android:layout_width="80dp"
           android:layout_height="80dp"
           android:src="@mipmap/ic_launcher"
           android:layout_centerVertical="true"
           android:layout_marginLeft="@dimen/dividing_10"
           />
       <TextView
           android:layout_toRightOf="@id/my_head"
           android:layout_centerVertical="true"
           android:layout_marginLeft="@dimen/dividing_10"
           android:id="@+id/my_name"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="清晨饮露"
           android:textSize="@dimen/textsize_16"
           />
       <TextView
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentRight="true"
           android:layout_centerVertical="true"
           android:layout_marginRight="20dp"
           android:text=">"
           android:textSize="30dp"
           />
   </RelativeLayout>

    <RelativeLayout
        android:id="@+id/my_layout_aboutme"
        android:layout_marginTop="@dimen/dividing_10"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white">
        <ImageView
            android:id="@+id/my_icon1"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_supervisor_account_black_24dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dividing_10"
            />
        <TextView
            android:layout_toRightOf="@id/my_icon1"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dividing_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="与我相关"
            android:textSize="@dimen/textsize_16"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:text=">"
            android:textSize="30dp"
            />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/my_layout_increase"
        android:layout_marginTop="@dimen/dividing_10"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white">
        <ImageView
            android:id="@+id/my_icon2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_note_add_black_24dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dividing_10"
            />
        <TextView
            android:layout_toRightOf="@id/my_icon2"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dividing_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="新增发布"
            android:textSize="@dimen/textsize_16"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:text=">"
            android:textSize="30dp"
            />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/my_layout_record"
        android:layout_marginTop="@dimen/dividing_10"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white">
        <ImageView
            android:id="@+id/my_icon3"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_query_builder_black_24dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dividing_10"
            />
        <TextView
            android:layout_toRightOf="@id/my_icon3"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dividing_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的发布"
            android:textSize="@dimen/textsize_16"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:text=">"
            android:textSize="30dp"
            />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/my_layout_intro"
        android:layout_marginTop="@dimen/dividing_10"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white">
        <ImageView
            android:id="@+id/my_icon4"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_touch_app_black_24dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dividing_10"
            />
        <TextView
            android:layout_toRightOf="@id/my_icon4"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dividing_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关于我们"
            android:textSize="@dimen/textsize_16"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:text=">"
            android:textSize="30dp"
            />
    </RelativeLayout>

</LinearLayout>
